# Test Environment Configuration
NODE_ENV=test

# Database
DATABASE_URL=postgresql://testuser:testpass@localhost:5432/testdb

# Redis
REDIS_URL=redis://localhost:6379

# JWT
JWT_SECRET=test-secret-key-for-testing-only
JWT_EXPIRES_IN=1h

# API
API_PORT=3001
API_PREFIX=api
API_VERSION=v1

# CORS
CORS_ORIGIN=http://localhost:3000

# Rate Limiting
RATE_LIMIT_TTL=60
RATE_LIMIT_MAX=100

# Logging
LOG_LEVEL=error

# Cache
CACHE_TTL=300

# Testing specific
TEST_TIMEOUT=30000
TEST_RETRIES=3
